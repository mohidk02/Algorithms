#include<iostream>
#include<bits/stdc++.h>
using namespace std;
void largest3(int arr[],int size)
{
    int first,second,third;
    first=second=third=0;
    for(int i=size-1;i>size-4;i--)
    {
        if(arr[i]>first)
        {
            third=second;
            second=first;
            first=arr[i];
        }
        else if(arr[i]>second)
        {
            third=second;
            second=arr[i];
        }
        else if(arr[i]>third)
        {
            third=arr[i];
        }
    }
    cout<<first<<" "<<second<<" "<<third<<endl;
}
int main()
{
    int arr[] = {10, 4, 3, 50, 23, 90};
    int n=sizeof(arr)/sizeof(arr[0]);
    largest3(arr,n);

    return 0;
}
